<% include ./partials/header.ejs %>

<div class="container">
	<div class="jumbotron">
		<h1>Welcome to Locate Places</h1>
		<h3>Sign Up</h3>
	</div>
	<div class="container">
		<form action="/register" class="col-md-5 col-md-offset-3" method="POST">
			<div class="form-group">
				<label for="user"> Username</label>
				<input type="name" class="form-control" id="user" name="username" placeholder="username">
			</div>
			<div class="form-group">
				<label for="password"> Password</label>
				<input type="password" class="form-control id="password" name="password" placeholder="password">
			</div>
			<button>Sign Up</button>
		</form>
	</div>
</div>

<% include ./partials/footer.ejs %>
